    "@svgr/webpack": "^8.0.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "latest",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "latest",
    "autoprefixer": "^10.4.16",
    "contentlayer": "^0.3.4",
    "date-fns": "^2.30.0",
    "dedent": "^0.7.0",
    "encoding": "^0.1.13",
