{
  "name": "iamsteve",
  "version": "8.0.0-beta",
  "private": true,
  "scripts": {
    "dev": "contentlayer dev & next dev",
    "sourcemap": "NODE_OPTIONS=--enable-source-maps next build",
    "build": "contentlayer build && next build && node ./scripts/generate-sitemap",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir app --dir components --dir lib --dir scripts",
    "icons-16": "create-react-icons -s 'public/icon/16/**.svg' -d 'components/icon/16' -t 'lib/icon-16.ejs'",
    "icons-24": "create-react-icons -s 'public/icon/24/**.svg' -d 'components/icon/24' -t 'lib/icon-24.ejs'",
    "icons": "yarn icons-16 && yarn icons-24"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "resolutions": {
    "@opentelemetry/api": "1.4.1",
    "@opentelemetry/core": "1.13.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "0.39.1",
    "@opentelemetry/resources": "1.13.0",
    "@opentelemetry/sdk-trace-base": "1.13.0",
    "@opentelemetry/sdk-trace-node": "1.13.0",
    "@opentelemetry/semantic-conventions": "1.13.0"
  },
  "dependencies": {
    "@jsdevtools/rehype-toc": "^3.0.2",
    "@netlify/plugin-nextjs": "^4.40.1",
    "@next/bundle-analyzer": "^13.4.10",
    "@radix-ui/react-accessible-icon": "^1.0.3",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-form": "^0.0.3",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-separator": "^1.0.3",
