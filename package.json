{
  "name": "iamsteve",
  "version": "8.1.20",
  "private": true,
  "packageManager": "pnpm@10.25.0",
  "scripts": {
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint app components lib --fix",
    "icons": "pnpm icons-16 && pnpm icons-24",
    "icons-16": "create-react-icons -s 'public/icon/16/**.svg' -d 'components/icon/16' -t 'lib/icon-16.ejs'",
    "preinstall": "npx only-allow pnpm",
    "serve": "next start",
    "dev": "NODE_NO_WARNINGS=1 contentlayer2 dev & next dev --turbo",
    "cl": "contentlayer2 build",
    "icons-24": "create-react-icons -s 'public/icon/24/**.svg' -d 'components/icon/24' -t 'lib/icon-24.ejs'",
    "sourcemap": "NODE_OPTIONS=--enable-source-maps next build",
    "analyze": "cross-env ANALYZE=true next build",
    "post": "node ./scripts/compose.js",
    "build": "contentlayer2 build && pnpm lint && next build",
    "validate:links": "node scripts/validate-collection-links.js",
    "validate:links:all": "CHECK_ALL=true node scripts/validate-collection-links.js",
    "prepare": "husky"
  },
  "browserslist": [
    "> 2%",
    "not dead",
    "not op_mini all"
  ],
  "pnpm": {
    "overrides": {
      "whatwg-url": "13.0.0"
    },
    "onlyBuiltDependencies": [
      "contentlayer2",
      "esbuild"
    ]
  },
  "dependencies": {
    "@jsdevtools/rehype-toc": "^3.0.2",
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@next/bundle-analyzer": "^14.2.33",
    "@next/mdx": "^15.5.7",
    "@radix-ui/react-accessible-icon": "^1.1.8",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-form": "^0.0.3",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.86.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.19",
    "clsx": "^2.1.1",
    "contentlayer2": "^0.5.8",
    "date-fns": "^2.30.0",
    "dedent": "^0.7.0",
    "encoding": "^0.1.13",
    "esbuild": "^0.25.0",
    "eslint-config-next": "^14.2.33",
    "feed": "^4.2.2",
    "github-slugger": "^2.0.0",
    "globby": "^11.1.0",
    "gray-matter": "^4.0.3",
    "inquirer": "^8.2.7",
    "marked": "^14.1.4",
    "mdast-util-to-string": "^4.0.0",
    "motion": "^12.23.25",
    "next": "16.0.10",
    "next-contentlayer2": "^0.5.8",
    "postcss": "^8.5.6",
    "postcss-nesting": "^12.1.5",
    "radix-ui": "^1.4.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-intersection-observer": "^9.16.0",
    "reading-time": "^1.5.0",
    "rehype": "^13.0.2",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-citation": "^1.0.3",
    "rehype-code-titles": "^1.2.1",
    "rehype-parse": "^8.0.5",
    "rehype-prism": "^2.3.3",
    "rehype-prism-plus": "^2.0.1",
    "rehype-slug": "^5.1.0",
    "rehype-stringify": "^9.0.4",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^10.1.0",
    "remark-smartypants": "^2.1.0",
    "sharp": "^0.32.6",
    "swr": "^1.3.0",
    "tailwind-merge": "^3.4.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@next/eslint-plugin-next": "^15.5.7",
    "@tailwindcss/postcss": "^4.1.18",
    "axios": "^1.13.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^4.2.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "p-limit": "^7.2.0",
    "prettier": "2.7.1",
    "sass": "^1.94.2",
    "tailwindcss": "^4.1.18"
  }
}
