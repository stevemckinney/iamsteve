<script>
document.addEventListener('DOMContentLoaded', function() {
    var sitemapUrl = 'https://saaspo.com/sitemap.xml'; // Your sitemap URL
    fetch(sitemapUrl)
    .then(response => {
        if (!response.ok) {
            throw new Error(`HTTP error! status: ${response.status}`);
        }
        return response.text();
    })
    .then(data => {
        var parser = new DOMParser();
        var xmlDoc = parser.parseFromString(data, "text/xml");

        // Check for parsing errors
        if (xmlDoc.getElementsByTagName('parsererror').length > 0) {
            throw new Error('Error parsing XML');
        }

        var urls = Array.from(xmlDoc.querySelectorAll('url loc')).map(node => node.textContent);

        // Check if any URLs were found
        if (urls.length === 0) {
            console.warn('No URLs found in the sitemap.');
            return;
        }

        // Filter URLs by a specific collection if necessary
        var collectionUrls = urls.filter(url => url.includes('/pages/'));

        if (collectionUrls.length === 0) {
            console.warn('No URLs found in the specified collection.');
            return;
        }

        var randomButtons = document.getElementsByClassName('random-page-button');
        Array.from(randomButtons).forEach(button => {
            button.addEventListener('click', function() {
                var randomUrl = collectionUrls[Math.floor(Math.random() * collectionUrls.length)];
                window.location.href = randomUrl;
            });
        });
    })
    .catch(error => console.error('Error fetching or parsing the sitemap:', error));
});
  
document.addEventListener('keydown', function(event) {
    const target = event.target;
    const isFormField = (
        target.tagName === 'INPUT' ||
        target.tagName === 'TEXTAREA' ||
        target.isContentEditable
    );

    if (isFormField) return;

    if (event.key === 'r') {
        event.preventDefault(); // Prevent the default action (e.g., page refresh)

        // Find all buttons with the class 'random-page-button'
        var buttons = document.getElementsByClassName('random-page-button');

        // Simulate a click on each button
        Array.from(buttons).forEach(button => {
            button.click();
        });
    }
});

